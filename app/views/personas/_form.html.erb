<%= simple_form_for @persona do |f| %>
  <%= f.input :cedula %>
  <%= f.input :nombre %>
  <%= f.input :estatura %>
  <%= f.input :estado %>
  <%= f.input :sexo %>
  <%= f.input :email %>

  <%= f.simple_fields_for :telefonos do |t| %>
    <%= t.input :telefono %>
  <% end %>

  <%= f.button :submit %>
<% end %>
